{
  "name": "@soatra/any",
  "version": "2.1.1",
  "description": "React Component for animating.",
  "repository": {
    "url": "https://github.com/johnsoatra/any"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "serve ./build",
    "build": "rollup -c rollup.config.ts",
    "rebuild": "rm -rf dist && rollup -c rollup.config.ts",
    "deploy": "NODE_ENV=production rm -rf dist && npm run build && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "ignorePatterns": [
      "src/App.tsx",
      "src/index.tsx"
    ]
  },
  "keywords": [
    "animation",
    "transition",
    "tailwind",
    "react"
  ],
  "author": "Soatra",
  "license": "MIT",
  "dependencies": {
    "tailwind-merge": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.23",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "react-scripts": "^5.0.1",
    "rollup": "^2.60.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-import-css": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^4.7.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./index.mjs": "./dist/index.mjs",
    "./static": "./dist/static/index.js",
    "./static/mjs": "./dist/static/index.mjs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
